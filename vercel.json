{
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@5.3.0"
    }
  },
  "rewrites": [
    { "source": "/assets/:path*", "destination": "/assets/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": true
}